@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="align-items-center">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Province List</h2>
        </div>
        <div class="col-6 text-right">
            <a class="btn btn-primary float-end" asp-action="Upsert"><i class="fas fa-plus"></i> &nbsp; Add New Province</a>
        </div>
    </div>

    <br />
    <div class="p-4 border rounded">
        <table id="tblData" class="table table-striped table-bordered pt-3" style="width:100%">
            <thead class="thead-dark">
                <tr class="table-dark">
                    <th>Name</th>
                    <th>Zip Code</th>
                    <th>City</th>
                    <th>Country or Region</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts {

    <script defer src="~/js/address/province.js"></script>

}
